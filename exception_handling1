import java.util.Scanner;
class Main
{
    public static void main(String[] args)
    {
        try {
            Scanner s=new Scanner(System.in);
            System.out.println("enter array size");
            int n=s.nextInt();
            int[] a=new int[n];
            System.out.println(n);
        }
        catch(NegativeArraySizeException e)
        {
            System.out.println(e);
        }
    }
}
